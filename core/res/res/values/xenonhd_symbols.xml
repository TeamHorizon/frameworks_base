<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2"> 
  <java-symbol type="dimen" name="nav_gesture_swipe_min_length" />
  <java-symbol type="dimen" name="nav_gesture_long_press_max_length" />
  <!-- Whether the device supports Smart Pixels -->
  <java-symbol type="bool" name="config_enableSmartPixels" />
  <java-symbol type="array" name="config_blockPackagesSensorDrain" />
  <!-- Ambient play -->
  <java-symbol type="string" name="ambient_recognition_notification" />
  <java-symbol type="string" name="ambient_recognition_information" />
  <!-- Screenrecord -->
  <java-symbol type="bool" name="config_enableScreenrecordChord" />
  <!-- Power menu -->
  <java-symbol type="drawable" name="ic_lock_screenrecord" />
  <!-- True if the notification's dynamic app icon and text tint should be optimised for dark
       backgrounds -->
  <java-symbol type="bool" name="config_useDarkBgNotificationIconTextTinting" />
  <java-symbol type="bool" name="config_useDarkBgNotificationTinting_override" />
  <java-symbol type="bool" name="config_notificationTinting_override" />

  <!-- Notification app name tint -->
  <java-symbol type="bool" name="config_allowNotificationAppNameTextTinting" />
</resources>
